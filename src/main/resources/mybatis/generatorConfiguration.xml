			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="false">
			<generatedKey column="id" sqlStatement="MySql" identity="true"/>
		</table>
		
		<!-- 资质 -->
		<table tableName="tb_aptitude" domainObjectName="TbAptitude"
